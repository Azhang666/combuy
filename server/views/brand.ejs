<aside class="col-2">
  <nav class="navSide">
    <ul>
      <% for( let i = 0; i < brand.length; i++ ) { %>
      <li class="brand">
        <img
          style="width: 20px"
          src="/public/images/brand/<%=brand[i].brand %>.png"
          alt=""
        />
        <a
          href="/product/<%=brand[i].brand_id%>"
          value="<%=brand[i].brand_id%>"
          title="<%=brand[i].brand%>"
          ><%=brand[i].brand%></a
        >
      </li>
      <% } %>
    </ul>
  </nav>
</aside>
